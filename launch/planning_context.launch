<launch>
    <!-- The semantic description (SRDF) that corresponds to the URDF -->
    <param name="robot_description_semantic" textfile="$(find millihex_moveit_config)/config/millihex.srdf" />

    <!-- Load default settings for kinematics; these settings are overridden by settings in a node's namespace -->
    <group ns="robot_description_kinematics">
        <rosparam command="load" file="$(find millihex_moveit_config)/config/kinematics.yaml"/>
    </group>
</launch>
